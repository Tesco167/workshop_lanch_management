<?xml version="1.0" encoding="UTF-8"?>
 <odoo>
    <data>
        <record id="test.github_form" model="ir.ui.view">
            <field name="name">test.github.form</field>
            <field name="model">test.github</field>
            <field name="arch" type="xml">
                <form string="test_git">
                    <field name="name"/>
                </form>
            </field>
        </record>

        <record model="ir.actions.act_window" id="test_git_list_action">
            <field name="name">test.list</field>
            <field name="res_model">test.github</field>
            <field name="view_mode">tree,form</field>
        </record>

        <menuitem id="main_menu" name="Test Main"/>

        <menuitem id="second_menu" name="Test Second"
                  parent="main_menu"
                  action="test_git_list_action"/>
    </data>
</odoo>